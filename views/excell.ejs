<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Patient/folder</title>

    <!-- Font Icon -->
    <link rel="stylesheet" href="../Assets/fonts/material-icon/css/material-design-iconic-font.min.css">

    <!-- Main css -->
    <link rel="stylesheet" href="../Assets/css/form.css">
</head>
<body>

    <div class="main">
        <div class="container">
            <div class="row">
                <div class="col-md-6 m-auto">
                    <h4 class="text-center display-4 my-4"> Uploads Excell </h4>
                   
                    <%if(FileInfo.length > 0){ %>
                        <% FileInfo.forEach(function(row){%>
                            <div>
                                <p><b>The file has been uploaded successfully. Check the details</b></p>
                                <p> The original name :<%= row.originalname%></p>
                                <p>Genrated File Name: <%= row.filename %></p>
                                <p>Generated Id :<%= row._id %></p>
                            </div>    
                    <form action="/dashboard/Excellfile/ <%= row.filename %>? method = POST"  method="POST" enctype="multipart/form-data">
                     <button class="btn btn-danger btn-block mt4">Click here complete </button>

                    </form>

                     <%});%>
                   <%}else{%>
                    <div>
                        <H3>no file uploaded </H3>  
                    </div>
                <% } %>

                </div>
            </div>
            <a href="/dashboard/UploadExcell" class="btn btn-success btn-sm active" role="button" aria-pressed="true">Click on this</a>

        </div>
             

    <!-- JS -->
    <script src="../Assets/vendor/jquery/jquery.min.js"></script>
    <script src="../Assets/js/formmain.js"></script>
    <script src="../Assets/js/showfile.js"></script>
</body><!-- This templates was made by Colorlib (https://colorlib.com) -->
</html>